t now!\r\n            </p>\r\n          ) : (\r\n            <Form size='large' onSubmit={handleFormSubmit}>\r\n              <Segment>\r\n                <Form.Input\r\n                  required\r\n                  fluid\r\n                  icon='user'\r\n                  iconPosition='left'\r\n                  placeholder='Your username'\r\n                  name='username'\r\n                  type='text'\r\n                  value={formState.username}\r\n                  onChange={handleChange}\r\n                />\r\n                <Form.Input\r\n                  required\r\n                  fluid\r\n                  icon='mail'\r\n                  iconPosition='left'\r\n                  placeholder='Your email'\r\n                  name='email'\r\n                  type='email'\r\n                  value={formState.email}\r\n                  onChange={handleChange}\r\n                />\r\n                <Form.Input\r\n                  required\r\n                  fluid\r\n                  icon='lock'\r\n                  iconPosition='left'\r\n                  placeholder='******'\r\n                  name='password'\r\n                  type='password'\r\n                  value={formState.password}\r\n                  onChange={handleChange}\r\n                />\r\n                <Button id='btnSubmit' fluid size='large' type='submit'>\r\n                  Submit\r\n                </Button>\r\n              </Segment>\r\n            </Form>\r\n          )}\r\n          {error && <div id='errorMsg'>{error.message}</div>}\r\n        </Grid.Column>\r\n      </Grid>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Signup;\r\n","C:\\Repos\\BootCamp\\Social-Stories\\client\\src\\components\\pages\\Navigation.js",[],"C:\\Repos\\BootCamp\\Social-Stories\\client\\src\\components\\pages\\Profile.js",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","severity":1,"message":"94","line":2,"column":10,"nodeType":