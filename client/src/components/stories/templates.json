[
  {
    "id": "t001",
    "title": "Transition template",
    "description": "Use this for transition",
    "pages": [
      {
        "id": "p001",
        "title": "Introduction",
        "description": "Introduction to student",
        "content": "<image src=\"https://cdn.decoratorist.com/wp-content/uploads/school-building-497644.jpg\" width: 200px/><br/><h3>Hello my name is {StudentName}. I am in grade {StudentGrade} this year.</h3>",
        
        "variables": [
          {
            "id": "v001",
            "name": "{StudentName}",
            "description": "Student's name here",
            "defaultValue": ""
          },
          {
            "id": "v002",
            "name": "{StudentGrade}",
            "description": "Student's grade here",
            "defaultValue": ""
          }
        ]
      },
      {
        "id": "p002",
        "title": "My current teacher",
        "description": "Introduction to student's teacher",
        "content": "My teacher this year is  {CurTeacherName}. Next year my teacher will be {NextTeacherName}.",
        "variables": [
          {
            "id": "v001",
            "name": "{CurTeacherName}",
            "description": "Current teacher's name here",
            "defaultValue": ""
          },
          {
            "id": "v002",
            "name": "{NextTeacherName}",
            "description": "Next year's teacher's name here",
            "defaultValue": ""
          }
        ]
      },
      {
        "id": "p003",
        "title": "My current classroom",
        "description": "Introduction to student's new class",
        "content": "My class this year is  {CurClassRoom}. Next year, my class will be {NextClassRoom}.",
        "variables": [
          {
            "id": "v001",
            "name": "{CurClassRoom}",
            "description": "Current class room name/description",
            "defaultValue": ""
          },
          {
            "id": "v002",
            "name": "{NextClassRoom}",
            "description": "Current class room name/description",
            "defaultValue": ""
          }
         
        ]
      },
      {
        "id": "p004",
        "title": "Additional details",
        "description": "Add any other relevant details here",
        "content": "{AddDetails}",
        "variables": [
          {
            "id": "v001",
            "name": "{AddDetails}",
            "description": "More about next year",
            "defaultValue": ""
          }
        ]
      }
    ]
  }
]
